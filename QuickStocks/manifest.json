{
  "manifest_version": 3,
  "name": "Quick Stocks",
  "version": "1.0.0",
  "description": "Provides financial data, news, and current stock prices for websites dedicated to certain multi-national companies",
  "action": {
    "default_icon": "/images/icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/images/icon16.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",   
    "https://*/*"   
  ]
}


